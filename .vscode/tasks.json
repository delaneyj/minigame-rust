{
    "version": "2.0.0",
    "command": "cargo",
    "presentation": {
        "echo": true,
        "reveal": "always",
        "focus": false,
        "panel": "shared",
        "showReuseMessage": true,
        "clear": false
    },
    "tasks": [
        {
            "label": "cargo build",
            "type": "shell",
            "args": [
                "build"
            ],
            "group": "build",
        },
        {
            "label": "cargo run",
            "type": "shell",
            "args": [
                "run"
            ]
        },
        {
            "label": "cargo test",
            "type": "shell",
            "args": [
                "test"
            ],
            "group": "test",
        }
    ]
}